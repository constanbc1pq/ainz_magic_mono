# ðŸ¤– Magic Gradio Proxy Configuration Example
# Copy this file to .env and update with your actual values

# Service Configuration
SERVICE_PORT=5719
SERVICE_HOST=0.0.0.0
ENVIRONMENT=development

# HuggingFace Space Configuration
HF_SPACE="nomad2082/magic-plus-1"
HF_TOKEN="your-huggingface-token-here"

# Backend Integration
BACKEND_URL="http://localhost:5720"
BACKEND_API_KEY=""

# CORS Configuration
ALLOWED_ORIGINS="http://localhost:3000,http://localhost:3001,http://localhost:5720"

# IP Whitelist Configuration (comma-separated)
ALLOWED_IPS="127.0.0.1,::1,localhost"

# File Processing Configuration
TEMP_DIR="/tmp/magic_proxy"
MAX_FILE_SIZE=104857600
SUPPORTED_FORMATS="obj,fbx,glb,gltf,ply,stl"

# Request Configuration
REQUEST_TIMEOUT=300
CONNECTION_TIMEOUT=30
MAX_RETRIES=3

# Logging Configuration
LOG_LEVEL="INFO"
LOG_FILE="logs/magic_proxy.log"
ENABLE_FILE_LOGGING=true

# Performance Configuration
WORKER_THREADS=4
MAX_CONCURRENT_REQUESTS=10

# Security Configuration
ENABLE_IP_WHITELIST=true
ENABLE_REQUEST_LOGGING=true