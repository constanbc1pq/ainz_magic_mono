# ðŸŽ¨ ArticulateHub Frontend Configuration Example
# Copy this file to .env and update with your actual values

# Backend API Configuration
REACT_APP_API_URL=http://localhost:5720

# App Configuration
REACT_APP_APP_NAME="ArticulateHub"
REACT_APP_VERSION="1.0.0"

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=104857600
REACT_APP_SUPPORTED_FORMATS=".obj,.fbx,.glb,.gltf,.ply,.stl"

# Feature Flags
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_ANALYTICS=false

# 3D Model Configuration
REACT_APP_DEFAULT_MODEL_PREVIEW=true
REACT_APP_PREVIEW_QUALITY="medium"

# UI Configuration
REACT_APP_THEME="light"
REACT_APP_LANGUAGE="zh-CN"